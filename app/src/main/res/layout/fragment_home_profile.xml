<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"><![CDATA[
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/linearL"
    tools:context="com.iust.panta.Profile_home"
    android:gravity="right"
    android:orientation="vertical">


    ]]>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/edittext"
        android:layout_gravity="right"
        android:layout_marginTop="20dp"
        android:background="#ff294067"
        android:contentDescription="@string/seprator_bar" />

    <EditText

        android:id="@+id/edittext"

        android:layout_width="match_parent"

        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"

        android:layout_marginTop="20dp"
        android:gravity="right"
        android:hint="@string/serch_eddit_text"
        android:singleLine="true">

    </EditText>

    <ExpandableListView
        android:id="@+id/Final_list"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/imageView"
        android:layout_gravity="right"
        android:layout_marginTop="10dp"

        android:groupIndicator="@null">

    </ExpandableListView>
</RelativeLayout>